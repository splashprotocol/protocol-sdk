{
  "name": "@splashprotocol/remote-sdk",
  "version": "2.0.0-beta.70",
  "type": "module",
  "description": "Api client of splash protocol",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "test": "jest"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "private": false,
  "dependencies": {
    "@splashprotocol/remote-builder": "2.0.0-beta.80",
    "@splashprotocol/api": "2.0.0-beta.57",
    "@splashprotocol/core": "2.0.0-beta.19",
    "@splashprotocol/browser-wallet": "1.0.0-beta.21"
  },
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "jest": "^29.7.0",
    "jest-node-exports-resolver": "^1.1.6",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.spec.ts"
    ]
  }
}
