{
  "name": "@splashprotocol/cml-builder",
  "version": "2.0.0-beta.130",
  "type": "module",
  "description": "Api client of splash protocol",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --clean",
    "test": "jest"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.cjs",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "private": false,
  "devDependencies": {
    "@types/jest": "^29.5.13",
    "@types/ramda": "^0.30.2",
    "jest": "^29.7.0",
    "jest-node-exports-resolver": "^1.1.6",
    "ts-jest": "^29.2.5",
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/*.spec.ts"
    ]
  },
  "dependencies": {
    "@dcspark/cardano-multiplatform-lib-browser": "^6.1.0",
    "@dcspark/cardano-multiplatform-lib-nodejs": "^6.1.0",
    "bip39": "^3.1.0",
    "hash-wasm": "^4.11.0",
    "int64-buffer": "^1.0.1",
    "ramda": "^0.30.1"
  },
  "peerDependencies": {
    "@splashprotocol/api": "2.0.0-beta.58",
    "@splashprotocol/core": "2.0.0-beta.19",
    "@splashprotocol/browser-wallet": "1.0.0-beta.22"
  }
}
